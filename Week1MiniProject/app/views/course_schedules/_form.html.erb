<%= form_with(model: @course_schedule, local: true) do |form| %>

    <div>
      <%= form.label :users_id, 'User' %>
      <%= form.collection_select :users_id, @users, :id, :display_name %>
    </div>
  
    <div>
      <%= form.label :quarter_name %><br>
      <%= form.text_field :quarter_name %><br>
    </div>
  
    <%= form.submit %>
  
  <% end %>
  
  <%= link_to 'Back to course schedules', course_schedules_path %>
  